{
  "expo": {
    "name": "lefty",
    "slug": "lefty",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.mfjkri.lefty"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Lefty to access your photos.",
          "cameraPermission": "Allow Lefty to access your camera"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "88f32de5-642e-4d16-956f-333c75c87ce3"
      }
    }
  }
}
